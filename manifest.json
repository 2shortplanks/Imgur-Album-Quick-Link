{
  "manifest_version": 2,
  "name": "Imager Album Quick Link",
  "short_name": "Search",
  "description": "Quickly load Imgur Album in a new incognito window",
  "version": "0.0.1",
  "minimum_chrome_version": "38",
  "permissions": [
    "contextMenus", 
    "tabs"],
  "icons": {
    "16": "assets/imgur.png"
  },
  "background": {
    "scripts": ["main.js"]
	}
}
